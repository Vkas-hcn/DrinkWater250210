<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="#E9F2FB"
    tools:context=".ui.setup.SetupFragment">

    <TextView
        android:text="Setting"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="#141A1E"
        android:gravity="center_vertical"
        android:paddingStart="20.5dp"
        android:layout_width="match_parent"
        android:layout_height="56dp"></TextView>
    
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginHorizontal="25dp"
        android:id="@+id/pp"
        app:cardElevation="0dp"
        app:cardCornerRadius="12dp"
        android:layout_marginTop="68dp"
        android:layout_height="58dp">
        
        <ImageView
            android:layout_width="30dp"
            android:src="@mipmap/setup_pp"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="16dp"
            android:layout_height="30dp"></ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:text="Privacy policy"
            android:textSize="14sp"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="54dp"
            android:textStyle="bold"
            android:textColor="#FF141A1E"
            android:layout_height="wrap_content"></TextView>
    </androidx.cardview.widget.CardView>

</FrameLayout>